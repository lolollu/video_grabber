<script type="text/javascript">
    $().ready(function() {
        $('.payBtn').click(function() {
			var $this = $(this);
			$("#payUsd").html($this.attr("usd"));
			$("#payRmb").html($this.attr("rmb"));
			$("#courseId").val($this.attr("courseId"));
            $('#payModal').modal();
			return false;
        });
		$('.payModalClose').click(function() {
			window.location.reload();
        });
    })
	
	$("#paySubmit").bind('click',function(){
		$('#payModal').modal('hide');
		$('#payTipModal').modal();
	});
	
	$('.payResultSuccessBtn').click(function() {
		window.location.href="/mc/order/list/buy";
	});
	
	$('.payResultFailedBtn').click(function() {
		window.location.reload();
	});
	
</script> 
<!-- Modal end-->
