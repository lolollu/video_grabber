<script type="text/javascript">
	function getCommentHtml(pageNumber){
		var $commentHtml = $("#commentHtml");
		$.ajax({
		url: "/comment/lesson/ea3af1b448ec861e0148ee3c5e87005b?pageNumber="+pageNumber,
		async: false,
		dataType: "html",
		success: function(data) {
			$commentHtml.html(data);
		}
		});
	}
	$().ready( function() {
		getCommentHtml(1);
	});
</script> 
<script type="text/javascript">
	$('#saveNoteBtn').click(function(e) {
		var noteTextarea = $("#noteTextarea").val();
		var $isPublic= $("#isPublic").val();
		var isp;
		if(isPublic.checked){
         	isp=true;
       	}else{
			isp=false;
		}
		$.ajax({
			url: "/mc/noteLesson/save",
			dataType: "json",
			type: 'post',
			data: {
				note: noteTextarea,
				isPublic: isp,
				lessonId:"ea3af1b448ec861e0148ee3c5e87005b"
			},
			contentType: "application/x-www-form-urlencoded; charset=utf-8",
			success: function(data) {
				if (data.status == "success") {
					$("#noteSaveTip").html("保存成功!").fadeIn().delay(2000).fadeOut();
				}
			}
		});
	});
</script> 
<script type="text/javascript">
	$('#saveCommentBtn').click(function(e) {
		var commentTextarea = $("#commentTextarea").val();
		$.ajax({
			url: "/mc/comment/ajaxSave",
			dataType: "json",
			type: 'post',
			data: {
				content: commentTextarea,
				typeId:"ea3af1b448ec861e0148ee3c5e87005b",
				commentType:"lesson"
			},
			contentType: "application/x-www-form-urlencoded; charset=utf-8",
			success: function(data) {
				if (data.status == "success") {
					$("#commentSaveTip").html("提问成功!").fadeIn().delay(2000).fadeOut();
					$("#commentTextarea").val("");
					getCommentHtml(1);
				} else {
					$("#commentSaveTip").html("参数错误!").fadeIn().delay(2000).fadeOut();
				}
			}
		});
	});
</script> 
<script type="text/javascript">
	 $().ready( function() {
		var $att = $(".att");
		$att.bind('click',function(){
			var $this = $(this);
			$.ajax({
				url: "/mc/friend/att",
				data: "id="+$this.val(),
				async: false,
				dataType: "json",
				beforeSend: function(data) {
					$this.html('请稍等...');
				},
				success: function(data) {
					$this.html(data.message);
					$this.attr("disabled",true); 
				}
			});
			return false;
		});
		var $attDelete = $(".attDelete");
		$attDelete.bind('click',function(){
			var $this = $(this);
			$.ajax({
				url: "/mc/friend/attDelete",
				data: "id="+$this.val(),
				async: false,
				dataType: "json",
				beforeSend: function(data) {
					$this.html('请稍等...');
				},
				success: function(data) {
					$this.html(data.message);
					$this.attr("disabled",true); 
				}
			});
			return false;
		});
	});
</script> 
<script type="text/javascript">
	window.setInterval(checkLogin, 60000);
	function checkLogin() {
		$.ajax({
			url: "/checkLogin?id=ea3af1b448ec861e0148ee3c5e87005b",
			async: false,
			dataType: "text",
			success: function(data) {
				if(data=="false"){
					alert("您的账号在其它终端登录,请确保账户安全,建议修改密码!");
					window.location.href="/login"; 
				}
			}
		});
	}
</script> 
